{
  "name": "automation-testing",
  "private": true,
  "version": "0.0.0",
  "description": "a Sails application",
  "keywords": [],
  "dependencies": {
    "@sailshq/lodash": "^3.10.3",
    "moment": "^2.24.0",
    "sails": "^1.2.3",
    "sails-hook-orm": "^2.1.1",
    "sails-hook-sockets": "^2.0.0",
    "sails-mysql": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "5.16.0",
    "chai-http": "^4.0.0",
    "mocha": "^6.1.4",
    "mock-require": "^3.0.0",
    "nyc": "^11.4.0",
    "sinon": "^4.1.3",
    "sinon-chai": "^3.0.0"
  },
  "nyc": {
    "exclude": [
      "api/responses/**",
      "test/**"
    ]
  },
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "test": "npm run lint && npm run custom-tests && echo 'Done.'",
    "lint": "eslint . --fix --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
    "custom-tests": "nyc --reporter=lcov --reporter=html ./node_modules/mocha/bin/mocha test/lifecycle.init.js test/integration/**/*.test.js",
    "ping": "nyc mocha test/lifecycle.init.js test/integration/ping/*.test.js"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/anonymous node/sails user/automation-testing.git"
  },
  "author": "anonymous node/sails user",
  "license": "",
  "engines": {
    "node": "^10.16"
  }
}
